{
  "name": "maps-app-javascript",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "precommit": "lint-staged",
    "build": "del dist && webpack --config webpack.build.config.js --env.NODE_ENV=local --env.production --progress && http-server . -p 9000 -g -d -o",
    "serve": "del dist && webpack --config webpack.config.js && webpack-dev-server --hot",
    "test": "del ~tmp && npm run copy:js && tsc --outDir './~tmp' && intern && del ~tmp"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "esri",
    "arcgis",
    "typescript"
  ],
  "author": "Rene Rubalcava <rrubalcava@odoe.net",
  "license": "Apache-2.0",
